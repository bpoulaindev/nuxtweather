{
  // https://nuxt.com/docs/guide/concepts/typescript
  "extends": "./.nuxt/tsconfig.json",
  "compilerOptions": {
    "module": "es2022",
    "types": [
      "cypress",
      "vitest/globals",
      "@testing-library/cypress"
    ],
    "paths": {
      "@utils/*": [
        "./utils/*"
      ],
      "@components/*": [
        "./components/*"
      ],
      "@stores/*": [
        "./stores/*"
      ],
      "@assets/*": [
        "./assets/*"
      ],
      "@": [
        "./"
      ]
    }
  }
}
